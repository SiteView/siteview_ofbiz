<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="ofbiz"
            name="ofbiz">
      </category>
      <view
            category="ofbiz"
            class="org.ofbiz.base.start.SampleView"
            icon="icons/sample.gif"
            id="org.ofbiz.base.start.SampleView"
            name="ofbiz">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
